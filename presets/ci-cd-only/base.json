{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "p3ol/renovate-presets//presets/common/base"
  ],
  "packageRules": [{
    // Do not use global enabledManagers as it cannot be reverted by packageRules
    "matchManagers": [
      "!dockerfile",
      "!helm-values",
      "!helmv3",
      "!github-actions"
    ],
    "enabled": false
  }, {
    "matchDepNames": ["node"],
    "enabled": true
  }]
}
