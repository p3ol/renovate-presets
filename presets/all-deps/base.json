{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "p3ol/renovate-presets//presets/common"
  ],
  "packageRules": [{
    "matchManagers": ["npm"],
    "schedule": ["before 9am every weekday"],
    "prConcurrentLimit": 10
  }, {
    "matchUpdateTypes": ["patch", "pin", "digest"],
    "matchManagers": ["npm"],
    "matchDepTypes": ["devDependencies"],
    "automerge": true
  }, {
    "matchDepTypes": ["devDependencies"],
    "matchManagers": ["npm"],
    "semanticCommitType": "chore",
    "prPriority": 0
  }, {
    "matchDepTypes": ["dependencies"],
    "matchManagers": ["npm"],
    "semanticCommitType": "fix",
    "prPriority": 10
  }, {
    "matchPackageNames": ["//^@?babel//"],
    "matchManagers": ["npm"],
    "groupName": "babel monorepo",
    "schedule": ["before 9am on monday"]
  }, {
    "matchPackageNames": ["//^@?jest//", "/^types/jest/"],
    "matchManagers": ["npm"],
    "groupName": "jest monorepo",
    "schedule": ["before 9am on monday"]
  }, {
    "matchPackageNames": [
      "/^webpack/",
      "/-webpack-plugin$/",
      "/@types/webpack/"
    ],
    "matchManagers": ["npm"],
    "groupName": "webpack monorepo",
    "schedule": ["before 9am on monday"]
  }, {
    "matchPackageNames": ["/^@storybook/"],
    "matchManagers": ["npm"],
    "groupName": "storybook monorepo",
    "schedule": ["before 9am on monday"]
  }, {
    "matchPackageNames": ["/^eslint/", "/^@typescript-eslint/"],
    "matchManagers": ["npm"],
    "groupName": "eslint monorepo",
    "schedule": ["before 9am on monday"]
  }, {
    "matchPackageNames": ["/^@poool/eslint/"],
    "matchManagers": ["npm"],
    "groupName": "poool eslint monorepo",
    "schedule": ["before 9am on monday"]
  }, {
    "matchPackageNames": ["/^@junipero/"],
    "matchManagers": ["npm"],
    "groupName": "poool junipero monorepo",
    "schedule": ["before 9am on monday"]
  }, {
    "matchPackageNames": ["/^@oakjs/"],
    "matchManagers": ["npm"],
    "groupName": "poool oak monorepo"
  }]
}
